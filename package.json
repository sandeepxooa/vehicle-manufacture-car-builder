{
  "name": "vehicle-manufacture-car-builder",
  "private": false,
  "author": "Hyperledger Composer",
  "homepage": "http://github.com/hyperledger/composer-sample-applications",
  "scripts": {
    "build": "ionic-app-scripts build",
    "clean": "ionic-app-scripts clean",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "licchk": "license-check-and-add",
    "test": "npm run licchk",
    "start": "node app.js"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "Dockerfile",
      "node_modules",
      "resources",
      "www",
      "src/assets/fonts",
      "plugins",
      "platforms"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [],
    "insert_license": false,
    "license_formats": {
      "editorconfig": {
        "eachLine": {
          "prepend": "# "
        }
      },
      "md": {
          "file": "../../markdown-license.txt"
      }
    }
  },
  "dependencies": {
    "@ionic-native/keyboard": "3.4.2",
    "config": "^1.26.1",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-console": "^1.1.0",
    "cordova-plugin-device": "^1.1.7",
    "cordova-plugin-ionic-keyboard": "^2.1.2",
    "cordova-plugin-ionic-webview": "^1.2.1",
    "cordova-plugin-splashscreen": "^4.1.0",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "express": "^4.15.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "shelljs": "^0.8.2",
    "ws": "^2.3.1"
  },
  "devDependencies": {
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/compiler-cli": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/forms": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@ionic-native/core": "3.4.2",
    "@ionic-native/splash-screen": "3.4.2",
    "@ionic-native/status-bar": "3.4.2",
    "@ionic/app-scripts": "1.3.7",
    "@ionic/cli-plugin-cordova": "1.0.0",
    "@ionic/cli-plugin-ionic-angular": "1.0.0",
    "@ionic/cloud-angular": "^0.12.0",
    "@ionic/storage": "2.0.1",
    "ionic-angular": "3.0.1",
    "ionicons": "3.0.0",
    "license-check-and-add": "~2.3.0",
    "rxjs": "5.1.1",
    "sw-toolbox": "3.4.0",
    "typescript": "2.4.0",
    "zone.js": "^0.8.4"
  },
  "version": "0.0.11",
  "description": "Vehicle Manufacture - A mobile application for the customers view",
  "license": "Apache-2.0",
  "cordova": {
    "plugins": {
      "cordova-plugin-console": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {}
    },
    "platforms": [
      "ios"
